    "demoAndDoc": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/demo-and-doc",
      "sourceRoot": "projects/demo-and-doc/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/demo-and-doc",
            "index": "projects/demo-and-doc/src/index.html",
            "browser": "projects/demo-and-doc/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/demo-and-doc/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-and-doc/src/favicon.ico",
              "projects/demo-and-doc/src/assets",
              {
                "glob": "**/*",
                "input": "dist/binom/sdk-core/assets",
                "output": "assets"
              }
              ,
              {
                "glob": "**/*",
                "input": "dist/binom/sdk-layout/assets",
                "output": "assets"
              } 
              ,
              {
                "glob": "**/*",
                "input": "dist/binom/sdk-theme/assets",
                "output": "assets"
              }
              ,
              {
                "glob": "**/*",
                "input": "dist/binom/sdk-prism/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/purple-green.css",
              "projects/demo-and-doc/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "demoAndDoc:build:production"
            },
            "development": {
              "buildTarget": "demoAndDoc:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "demoAndDoc:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/demo-and-doc/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/demo-and-doc/src/favicon.ico",
              "projects/demo-and-doc/src/assets"
            ],
            "styles": [
              "projects/demo-and-doc/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
 